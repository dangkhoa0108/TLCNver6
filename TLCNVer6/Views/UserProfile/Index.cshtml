
@{
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js before-run" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="bootstrap admin template">
    <meta name="author" content="">

    <title>Thông Tin Cá Nhân</title>

    <link rel="apple-touch-icon" href="~/assest/images/apple-touch-icon.png">
    <link rel="icon" href="images/icon/fav_icon.png">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/assest/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assest/css/bootstrap-extend.min.css">
    <link rel="stylesheet" href="~/assest/css/site.min.css">

    <link rel="stylesheet" href="~/assest/vendor/animsition/animsition.css">
    <link rel="stylesheet" href="~/assest/vendor/asscrollable/asScrollable.css">
    <link rel="stylesheet" href="~/assest/vendor/switchery/switchery.css">
    <link rel="stylesheet" href="~/assest/vendor/intro-js/introjs.css">
    <link rel="stylesheet" href="~/assest/vendor/slidepanel/slidePanel.css">
    <link rel="stylesheet" href="~/assest/vendor/flag-icon-css/flag-icon.css">

    <!-- Plugin -->
    <link rel="stylesheet" href="~/assest/vendor/chartist-js/chartist.css">
    <link rel="stylesheet" href="~/assest/vendor/jvectormap/jquery-jvectormap.css">

    <!-- Page --

    <!-- Fonts -->
    <link rel="stylesheet" href="~/assest/fonts/web-icons/web-icons.min.css">
    <link rel="stylesheet" href="~/assest/fonts/brand-icons/brand-icons.min.css">
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,300italic'>
    <link href="css/preloader.css" rel="stylesheet" type="text/css">

    <!-- Scripts -->
    <script src="~/assest/vendor/modernizr/modernizr.js"></script>
